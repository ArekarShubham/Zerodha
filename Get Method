package selenium;

import org.openqa.selenium.WebDriver;
import org.openqa.selenium.chrome.ChromeDriver;

public class GetTitle {
	public static void main(String[] args)  {
		System.setProperty("webdriver.chrome.driver",
				"D:\\Testing_Application_Software\\Softwares\\chromedriver_win32\\chromedriver.exe");
		
		WebDriver driver = new ChromeDriver();
		driver.get("https://www.facebook.com/signup");
		
		String title = driver.getTitle();
		System.out.println(title);

	

	}
	public class Configuration1 {
	
	public static String driverPath = "D:\\Testing_Application_Software\\Softwares\\chromedriver_win32\\chromedriver.exe";
	
	public static String appUrl = "https://www.facebook.com/signup";
	
	public static String screenshotPath = "D:\\Testing_Application_Software\\Softwares\\eclipse-jee-photon-R-win32-x86_64\\Screenshot\\image_";
	
	public static String excelPath = "D:\\Testing_Application_Software\\Softwares\\eclipse-jee-photon-R-win32-x86_64\\Testing.xlsx";
}
}
